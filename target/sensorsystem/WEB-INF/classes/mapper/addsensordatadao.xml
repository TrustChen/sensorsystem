<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="org.sensorsystem.dao.sensordao.sensordatadao">

    <insert id="insertSensordata" parameterType="sensordata">

        insert into
        sensordata (name, date,time,data,unit,sid) values(#{name},#{date}, #{time}, #{data},#{unit},#{sid})

    </insert>


</mapper>