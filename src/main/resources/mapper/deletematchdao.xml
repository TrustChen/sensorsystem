<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="org.sensorsystem.dao.sensordao.matchdeletedao">

    <insert id="deletematch" parameterType="sensormatch">

        delete from sensormatch
        where
        uid = #{uid} AND sid = #{sid}

    </insert>


</mapper>