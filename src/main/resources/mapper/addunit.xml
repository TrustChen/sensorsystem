<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="org.sensorsystem.dao.unitdao.unitdao">

    <insert id="insertunit" parameterType="unitconversion">

        insert into
        unitconversion (unit, rate, m,kg,s,a,k,mol,cd)
        values
        (#{unit}, #{rate}, #{m},#{kg},#{s},#{a},#{k},#{mol},#{cd})

    </insert>



</mapper>